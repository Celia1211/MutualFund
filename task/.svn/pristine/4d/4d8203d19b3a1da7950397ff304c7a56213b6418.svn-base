
<jsp:include page="template-header.jsp" />

<div class="col-lg-10">
	<h3>Create Fund</h3>
	<form method="post" action="createFund"
		class="form-horizontal col-lg-5">
		<div class="form-group">
			<label class="col-lg-5 control-label"> Fund Name:</label>
			<div class="col-lg-7">
				<input type="text" class="form-control" placeholder="Fund Name"
					name="fundName" />
			</div>
		</div>
		<div class="form-group">
			<label class="col-lg-5 control-label"> Fund Ticker:</label>
			<div class="col-lg-7">
				<input type="text" class="form-control" placeholder="Fund Ticker"
					name="fundTicker" />
			</div>
		</div>

		<div class="form-group">
			<label class="col-lg-5 control-label"> Default Price:</label>
			<div class=" col-lg-7">
			<div class=" input-group">
				<div class="input-group-addon">$</div>
				<input type="text" class="form-control" placeholder="0.00"
					name="price" />
			</div>
			</div>
		</div>
		<div class="form-group">
			<label class="col-lg-5 control-label"> Total Share Amount:</label>
			<div class="col-lg-7">
						<div class=" input-group">
				<input type="text" class="form-control"
					placeholder="Total Share Amount" name="100" />
					<div class="input-group-addon">.00</div>
					</div>
			</div>
		</div>

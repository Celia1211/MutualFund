
<jsp:include page="template-header.jsp" />

<div class="col-lg-10">
	<h3>Create Employee Account</h3>
	<form method="post" action="createEmployee"
		class="form-horizontal col-lg-5">
		<div class="form-group">
			<label class="col-lg-5 control-label"> User Name:</label>
			<div class="col-lg-7">
				<input type="text" class="form-control" placeholder="User Name"
					name="userName" />
			</div>
		</div>
		<div class="form-group">
			<label class="col-lg-5 control-label"> First Name:</label>
			<div class="col-lg-7">
				<input type="text" class="form-control" placeholder="First Name"
					name="firstName" />
			</div>
		</div>
		<div class="form-group">
			<label class="col-lg-5 control-label"> Last Name:</label>
			<div class="col-lg-7">
				<input type="text" class="form-control" placeholder="Last Name"
					name="lastName" />
			</div>
		</div>
	  <div class="form-group">
			<label class="col-lg-5 control-label"> Email:</label>
			<div class="col-lg-7">
				<input type="text" class="form-control" placeholder="xinyanliu@gmail.com"
					name="email" />
			</div>
		</div>
	   <div class="form-group">
			<label class="col-lg-5 control-label"> Password:</label>
			<div class="col-lg-7">
				<input type="password" class="form-control" placeholder="at least 6 digit length"
					name="password" />
			</div>
		</div>
			 <div class="form-group">
			<label class="col-lg-5 control-label"> Confirm Password:</label>
			<div class="col-lg-7">
				<input type="password" class="form-control" placeholder="repeat password"
					name="confirmPassword" />
			</div>
		</div>
		<div class="form-group">
			<div class="col-sm-offset-3 col-sm-3">
				<button type="submit" class="btn btn-primary">Submit</button>
			</div>
			<div class="col-sm-offset-1 col-sm-5">
				<button type="reset" class="btn btn-primary">Reset</button>
			</div>
		</div>
	</form>
</div>

<jsp:include page="template-footer.jsp" />